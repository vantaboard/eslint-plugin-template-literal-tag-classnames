{
    "name": "eslint-plugin-template-literal-tag-classname",
    "version": "0.0.1",
    "description": "Provides linting for classnames constructed using a tag.",
    "engines": {
        "node": ">=8.10.0"
    },
    "main": "dist/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "prebuild": "yarn run clean",
        "build": "tsc",
        "lint": "eslint **/*.ts",
        "lint:ci": "yarn run eslint -- --quiet",
        "typecheck": "tsc --noEmit",
        "fix": "yarn run eslint -- --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "release": "semantic-release"
    },
    "peerDependencies": {},
    "dependencies": {
        "@typescript-eslint/experimental-utils": "^4.20.0",
        "eslint-plugin-prettier": "^5.0.0"
    },
    "devDependencies": {
        "@swc-node/register": "^1.6.6",
        "@swc/core": "^1.3.74",
        "@swc/jest": "^0.2.27",
        "@types/eslint": "^7.2.8",
        "@types/jest": "^26.0.22",
        "@types/node": "^20.4.7",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "eslint": "^8.46.0",
        "eslint-config-prettier": "^8.10.0",
        "jest": "^29.6.2",
        "prettier": "^3.0.1",
        "rimraf": "^3.0.2",
        "typescript": "^5.0.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vantaboard/eslint-plugin-template-literal-tag-classname.git"
    },
    "keywords": [
        "typescript",
        "template",
        "eslint",
        "eslintplugin",
        "eslint-plugin",
        "tag",
        "template-literal",
        "template-literal-tag",
        "tailwind",
        "tailwindcss"
    ],
    "author": {
        "name": "Brighten Tompkins",
        "email": "brightenqtompkins@gmail.com",
        "url": "https://brightentompkins.com/"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/vantaboard/eslint-plugin-template-literal-tag-classname/issues"
    },
    "homepage": "https://github.com/vantaboard/eslint-plugin-template-literal-tag-classname#readme"
}
